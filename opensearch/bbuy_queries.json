{
  "settings": {
    "index.refresh_interval": "5s"
  },
  "mappings" : {
    "properties" : {
      "@timestamp" : {
        "type" : "date"
      },
      "@version" : {
        "type" : "text",
        "fields" : {
          "keyword" : {
            "type" : "keyword",
            "ignore_above" : 256
          }
        }
      },
      "category" : {
        "type" : "text",
        "fields" : {
          "keyword" : {
            "type" : "keyword",
            "ignore_above" : 256
          }
        }
      },
      "click_time" : {
        "type" : "date",
        "format": "yyyy-MM-dd HH:mm:ss[.SSS][.SS][.S] || strict_date_optional_time"
      },
      "query" : {
        "type" : "text",
        "analyzer":  "english",
        "fields" : {
          "keyword" : {
            "type" : "keyword",
            "ignore_above" : 256
          }
        }
      },
      "query_time" : {
        "type" : "date",
        "format": "yyyy-MM-dd HH:mm:ss[.SSS][.SS][.S] || strict_date_optional_time"

      },
      "sku" : {
        "type" : "text",
        "fields" : {
          "keyword" : {
            "type" : "keyword",
            "ignore_above" : 256
          }
        }
      },
      "user" : {
        "type" : "text",
        "fields" : {
          "keyword" : {
            "type" : "keyword",
            "ignore_above" : 256
          }
        }
      }
    }
  }
}
